// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "clang.executable": "/usr/bin/clang",
    "clang.completion.enable": true,
    "clang.cxxflags": [
        "-std=c++11","-I/path/to/my/include/directory"
    ],
    "files.associations": {
        "initializer_list": "cpp",
        "utility": "cpp",
        "algorithm": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "array": "cpp"
    }
}