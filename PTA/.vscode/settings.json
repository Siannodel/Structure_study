// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "clang.executable": "/usr/bin/clang",
    "clang.completion.enable": true,
    "clang.cxxflags": [
        "-std=c++11","-I/path/to/my/include/directory"
    ],
    "files.associations": {
        "initializer_list": "cpp",
        "utility": "cpp",
        "algorithm": "cpp"
    }
}